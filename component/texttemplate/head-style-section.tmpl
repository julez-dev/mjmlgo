<!--[if !mso]><!-->
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<!--<![endif]-->
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<style type="text/css">
  #outlook a { padding:0; }
  body { margin:0;padding:0;-webkit-text-size-adjust:100%;-ms-text-size-adjust:100%; }
  table, td { border-collapse:collapse;mso-table-lspace:0pt;mso-table-rspace:0pt; }
  img { border:0;height:auto;line-height:100%; outline:none;text-decoration:none;-ms-interpolation-mode:bicubic; }
  p { display:block;margin:13px 0; }
</style>
<!--[if mso]>
<noscript>
<xml>
<o:OfficeDocumentSettings>
  <o:AllowPNG/>
  <o:PixelsPerInch>96</o:PixelsPerInch>
</o:OfficeDocumentSettings>
</xml>
</noscript>
<![endif]-->
<!--[if lte mso 11]>
<style type="text/css">
  .mj-outlook-group-fix { width:100% !important; }
</style>
<![endif]-->
{{range $font, $href := .Fonts}}
<!--[if !mso]><!-->
<link href="{{$href}}" rel="stylesheet" type="text/css">
<style type="text/css">
    @import url({{$href}});
</style>
<!--<![endif]-->
}
{{end}}
{{ if .MJMLStyles }}
    <style type="text/css">
      @media only screen and (min-width:{{.Breakpoint}}) {
            {{range $className, $rules := .MJMLStyles}}.{{$className}} {
                  {{range $rules}}{{.}};
                  {{end}}
            }
            {{end}}
      }
    </style>
    <style media="screen and (min-width:{{.Breakpoint}})">
        {{range $className, $rules := .MJMLStyles}}.moz-text-html .{{$className}} {
                {{range $rules}}{{.}};
                {{end}}
        }
        {{end}}
    </style>
    {{ if .PrinterSupport }}
    <style type="text/css">
      @media only print {
            {{range $className, $rules := .MJMLStyles}}.{{$className}} {
                  {{range $rules}}{{.}};
                  {{end}}
            }
            {{end}}
      }
    </style>
    {{ end }}
    {{ if .ForceOWADesktop }}
    <style type="text/css">
      @media only print {
          {{range $className, $rules := .MJMLStyles}}[owa] .{{$className}} {
                {{range $rules}}{{.}};
                {{end}}
          }
          {{end}}
      }
    </style>
    {{end}}
{{end}}
{{ if .IncludeMobileFullWidthStyle }}
<style type="text/css">
    @media only screen and (max-width:{{.LowerBreakpoint}}) {
        table.mj-full-width-mobile { width: 100% !important; }
        td.mj-full-width-mobile { width: auto !important; }
    }
    {{ end }}
</style>
{{ if .UserStyles }}
<style type="text/css">
{{ range .UserStyles }}
{{ range .MediaRules }}{{ .Condition }} {
    {{ range .Rules }}{{ .Selectors }} {
        {{ range .Declarations }}{{ .Property }}: {{ .Value }}{{ if .Important }} !important{{ end }};
        {{ end }}
    }
    {{end}}
}
{{end}}
{{ range .Rules }}
    {{ .Selectors }} {
        {{ range .Declarations }}{{ .Property }}: {{ .Value }}{{ if .Important }} !important{{ end }};
        {{ end }}
    }
{{end}}
{{end}}
</style>
{{end}}
